extends ../layout

block content

	form(method="post", action="insert")
		div.input
			label Křestní
			input.title(name="first_name")
		div.input
			label Příjmení
			input.title(name="last_name")
		div.input
			label e-mail
			input.title(name="email")
		div.input
			label výška
			input.title(name="height")
		input(type="submit" value="Odeslat data")